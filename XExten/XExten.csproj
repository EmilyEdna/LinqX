<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>EmilyEdna</Authors>
    <Product />
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Description>This package extends LINQ and produces anonymous expressions. For example: new {t1 = xx, T2 = xx}
projectUrl:https://github.com/EmilyEdna/XExten</Description>
    <PackageProjectUrl>https://github.com/EmilyEdna/XExten</PackageProjectUrl>
    <PackageTags>linq</PackageTags>
    <RepositoryUrl>https://github.com/EmilyEdna/XExten</RepositoryUrl>
    <PackageReleaseNotes>https://github.com/EmilyEdna/XExten</PackageReleaseNotes>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DocumentationFile></DocumentationFile>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DocumentationFile>D:\Project\XExten\XExten\XExten.xml</DocumentationFile>
  </PropertyGroup>
  <ItemGroup>
    <Compile Remove="DynamicType\**" />
    <EmbeddedResource Remove="DynamicType\**" />
    <None Remove="DynamicType\**" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="12.0.1" />
  </ItemGroup>
